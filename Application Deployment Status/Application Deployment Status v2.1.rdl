    <DataSet Name="CollectionInfo">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@AppInfo">
            <Value>=Parameters!AppInfo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT
       [aa].[CollectionID],
       [aa].[CollectionName]
       FROM [v_ApplicationAssignment] [aa]
       WHERE [aa].[ApplicationName] like N'%'+@AppInfo+N'%'
